# https://leetcode.com/problems/binary-search/
class Solution:
    def search(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: int
        """
        #for i in range(0,len(nums)):
        if target in nums:
            global final 
            final=nums.index(target)
        else :
            return -1
            
        return final
