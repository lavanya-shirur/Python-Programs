# https://leetcode.com/problems/reverse-words-in-a-string-iii/
class Solution:
    def reverseWords(self, s):
        """
        :type s: str
        :rtype: str
        """
        new=[]
        final=[]
        new=s.split(" ")
        for ne in new:
            final.append(ne[::-1])
            output=' '.join(final)
        return(output)
