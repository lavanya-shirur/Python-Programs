#2.two for loops
def string_unique(arr):
    arr=str(arr)
    unique = True
    for i in range(len(arr)):
        for j in range(i+1,len(arr)):
            print(arr[i],arr[j])
            if arr[i] == arr[j]:
                unique=False
    return unique

print(string_unique("laavy"))

