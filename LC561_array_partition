import itertools
class Solution:
    def arrayPairSum(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        #nums=set(nums)
        #newlist=[]
        #size=int(len(nums)/2)
        return sum(sorted(nums)[::2])
        
