def fibo(N):
    if N==0:
        return 0
    if N==1:
        return 1
    if N>1:
        return fibo(N-1)+fibo(N-2)

class Solution:
    def fib(self, N):
        """
        :type N: int
        :rtype: int
        """
        if N==0:
            return 0
        if N==1:
            return 1
        if N>1:
            return fibo(N)
      
            
