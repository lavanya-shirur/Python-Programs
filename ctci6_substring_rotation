def issubstring(s1,s2):
    res=[]
    for i in range(len(s1)):
        for length in range(1,len(s1)-i):
            res.append(s1[i:i+length])
    print(res)

    #if s2 in res:
    #    return True
    #else:
    #    return False
    if s2 in (s1+s1):
        return True
    else:
        return False

print(issubstring("lavanya","anyalav"))
