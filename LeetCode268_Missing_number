# https://leetcode.com/problems/missing-number/
class Solution(object):
    def missingNumber(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        partial=(sum(nums))
       
        maxele=len(nums)
                    
        total=(maxele*(maxele+1))/2
      
        return(total-partial)
        
